{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "dev": {
        "cache": false,
        "dependsOn": ["^dev"],
        "outputs": []
      },
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "dist/**"]
      },
      "start": {
        "dependsOn": ["build"]
      }
    }
  }
  